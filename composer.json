{
    "name": "justimmo/php-sdk",
    "description": "PHP-SDK for the JUSTIMMO API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "doctrine/annotations": "^1.13",
        "guzzlehttp/guzzle": "^6.0",
        "kevinrob/guzzle-cache-middleware": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.17"
    },
    "autoload": {
        "psr-4": {
            "Justimmo\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Justimmo\\Api\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
